"greeting" [ "Hello world!" ] define
"greet"    [ greeting print ] define
greet

"ascii-0"       [ 47                ] define
"print-if-even" [ 2 % [print] [] if ] define
"offset-1"      [ dup 1 +           ] define
"print-even-0-to-8" [ 
        ascii-0
        9 [ 1 + dup offset-1 print-if-even ] loop 
] define

print-even-0-to-8
